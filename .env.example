# 火山引擎API Configuration
# 请将以下配置复制到 .env 文件中，并替换为实际的值
VITE_VOLCENGINE_ACCESS_KEY_ID=your_volcengine_access_key_id_here
VITE_VOLCENGINE_SECRET_ACCESS_KEY=your_volcengine_secret_access_key_here
VITE_VOLCENGINE_SECURITY_TOKEN=your_volcengine_security_token_here

# 腾讯云OSS Configuration
# 请将以下配置复制到 .env 文件中，并替换为实际的值
VITE_COS_SECRET_ID=your_cos_secret_id_here
VITE_COS_SECRET_KEY=your_cos_secret_key_here
VITE_COS_REGION=your_cos_region_here
VITE_COS_BUCKET=your_cos_bucket_here
VITE_COS_DOMAIN=your_cos_domain_here # 例如：bucket-name.cos.region.myqcloud.com